{
  "name": "geosense-plus-webapp",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve app-main",
    "build:all": "ng build lib-ui && ng build lib-core && ng build app-main",
    "watch": "ng build app-main --watch --configuration development",
    "watch:ui": "ng build lib-ui --watch --configuration development",
    "watch:core": "ng build lib-core --watch --configuration development",
    "test": "ng test app-main",
    "story": "ng run lib-ui:storybook",
    "build-story": "ng run lib-ui:build-storybook"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "~20.1.5",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/material": "~20.1.5",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.1.0",
    "@angular/build": "^20.1.4",
    "@angular/cli": "^20.1.4",
    "@angular/compiler-cli": "^20.1.0",
    "@storybook/addon-docs": "9.1.1",
    "@storybook/addon-onboarding": "9.1.1",
    "@storybook/angular": "9.1.1",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.2.0",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.1.0",
    "storybook": "9.1.1",
    "typescript": "~5.8.2"
  }
}